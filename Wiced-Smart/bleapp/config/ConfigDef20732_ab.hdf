            enum
            {
                {0, "Once per scan cycle"},
                {1, "Once per 2 scan cycles"},
                {2, "Once per 4 scan cycles"},
                {3, "Once per 8 scan cycles"},
                {4, "Once per 16 scan cycles"},
                {5, "Once per 32 scan cycles"},
                {6, "Once per 64 scan cycles"},
                {7, "Once per 128 scan cycles"}
